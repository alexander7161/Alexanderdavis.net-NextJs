		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "^7.10.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-jest": "^24.0.2",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.3",
		"jest": "23",
		"jest-styled-components": "7.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"ts-jest": "23.10.5",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^4.0.3"
	}

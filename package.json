	"scripts": {
		"start": "npm run dev",
		"dev": "next src/app",
		"test": "jest --coverage",
		"tsc": "run-p tsc-app tsc-functions",
		"tsc-app": "tsc --project src/app --noEmit",
		"tsc-functions": "tsc --project src/functions --noEmit",
		"test:ci": "CI=true react-scripts test --env=jsdom",
		"cov": "npm run test -- --coverage",
		"clean": "rimraf dist",
		"lint": "eslint src/**/*.{ts,tsx}",
		"build-functions": "tsc --project src/functions",
		"build-app": "next build src/app",
		"copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
		"install-deps": "cd dist/functions && yarn",
		"build-public": "cpx src/public/**/*.* dist/public -C",
		"deploy": "firebase deploy",
		"build-prod": "run-s build-functions build-app build-public copy-deps install-deps"
	},
	"devDependencies": {
		"@testing-library/react": "^11.0.4",
		"@types/classnames": "^2.2.10",

{
	"extends": ["tslint:latest", "tslint-react"],
	"rules": {
		"jsx-wrap-multiline": false,
		"indent": [true, "tabs", 4],
		"interface-name": [true, "never-prefix"],
		"object-literal-sort-keys": false,
		"jsx-no-multiline-js": false,
		"no-implicit-dependencies": [true, "dev"],
		"trailing-comma": false
	}
}
